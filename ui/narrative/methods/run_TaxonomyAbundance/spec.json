{
  "ver" : "",
  "authors" : ["james_verasoto"],
  "contact" : "http://kbase.us/contact-us/",
  "visible" : true,
  "categories" : ["active","util"],
  "widgets": {
    "input": "kbaseNarrativeMethodInput",
    "output" : "no-display"
  },
  "parameters" : [
    {
      "id" : "amplicon_matrix_ref",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {
        "valid_ws_types" : ["KBaseMatrices.AmpliconMatrix"]
      }
    },
    {
      "id" : "test_row_attri_ref",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {
        "valid_ws_types" : ["KBaseExperiments.AttributeMapping"]
      }
    },
    {
      "id" : "attri_mapping_ref",
      "optional" : true,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {
        "valid_ws_types" : ["KBaseExperiments.AttributeMapping"]
      }
    },
    {
      "id": "threshold",
      "optional": false,
      "advanced": false,
      "allow_multiple": false,
      "default_values": [ "0.005" ],
      "field_type": "text",
      "text_options": {
        "validate_as": "float",
        "min_float" : 0.0,
        "max_float" : 1.0
      }
    },
    {
      "id" : "taxonomy_level",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [],
      "field_type" : "dropdown",
      "dropdown_options":
      {
        "options":
        [
          {
              "display": "1: Domain",
              "value": 1
          },
          {
              "display": "2: Phylum",
              "value": 2
          },
          {
              "display": "3: Class",
              "value": 3
          },
          {
              "display": "4: Order",
              "value": 4
          },
          {
              "display": "5: Family",
              "value": 5
          },
          {
              "display": "6: Genus",
              "value": 6
          }
        ]
      }
    },
    {
      "id": "meta_group",
      "optional": false,
      "advanced": true,
      "allow_multiple": false,
      "default_values": [ "" ],
      "field_type": "textsubdata",
      "textsubdata_options": {
        "subdata_selection": {
          "parameter_id": "attri_mapping_ref",
          "subdata_included": ["attributes/[*]/attribute"],
          "path_to_subdata": ["attributes"],
          "selection_id": "attribute"
        },
        "multiselection": false,
        "show_src_obj": false,
        "allow_custom": false
      }
    }
  ],
  "parameter-groups": [
    {
      "id": "grouping_label",
      "parameters": [
        "meta_group"
      ],
      "optional": true,
      "allow_multiple": false,
      "with_border": false,
      "advanced" : false
    }
  ],
  "behavior" : {
    "service-mapping" : {
      "url" : "",
      "name" : "TaxonomyAbundance",
      "method" : "run_TaxonomyAbundance",
      "input_mapping" : [
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        },
        {
          "input_parameter" : "amplicon_matrix_ref",
          "target_property" : "amplicon_matrix_ref",
          "target_type_transform": "resolved-ref"
        },
        {
          "input_parameter" : "test_row_attri_ref",
          "target_property" : "test_row_attri_ref",
          "target_type_transform": "resolved-ref"
        },
        {
          "input_parameter" : "attri_mapping_ref",
          "target_property" : "attri_mapping_ref",
          "target_type_transform": "resolved-ref"
        },
        {
          "input_parameter" : "threshold",
          "target_property" : "threshold"
        },
        {
          "input_parameter" : "taxonomy_level",
          "target_property" : "taxonomy_level"
        },
        {
          "input_parameter" : "grouping_label",
          "target_property" : "grouping_label"
        }
      ],
      "output_mapping" : [
        {
          "narrative_system_variable": "workspace",
          "target_property": "wsName"
        },
        {
            "service_method_output_path": [0, "report_name"],
            "target_property": "report_name"
        },
        {
            "service_method_output_path": [0, "report_ref"],
            "target_property": "report_ref"
        },
        {
            "constant_value": "16",
            "target_property": "report_window_line_height"
        }
      ]
    }
  },
  "job_id_output_field": "docker"
}